extends sidebarbase

block title
    title #{object}

block headstyle
    style.
        .item-wrapper {
            float:left;
            width: 100%;
        }

        .item {
            width:200px;
            margin:4px;
            display: inline-block;
        }

block maincontent
    +heading("Services", object, namespace, documentation)

    if Object.keys(services).length > 0
        h3 Services
        each service in Object.keys(services)
            +operationHeader(service, services[service])

    p &nbsp;

    a(href='../index.html') Back
